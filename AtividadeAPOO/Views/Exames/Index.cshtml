@model IEnumerable<Modelo.Cadastros.Exame>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Novo Exame", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Descricao)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Descricao)
            </td>
            <td>
                @Html.ActionLink("Editar Exame", "Edit", new { id=item.ExameId }) |
                @Html.ActionLink("Detalhes", "Details", new { id=item.ExameId }) |
                @Html.ActionLink("Excluir Exame", "Delete", new { id=item.ExameId })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
